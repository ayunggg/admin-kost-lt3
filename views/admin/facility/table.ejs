<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Fasilitas</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < datasFacility.length; i++) {%>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= datasFacility[i].facility %></td>
      <td>
        <form
          action="/facility/delete/<%= datasFacility[i].id %>?_method=DELETE"
          method="POST"
        >
          <a
            href="/facility/edit/<%= datasFacility[i].id %>"
            class="btn btn-warning"
          >
            <i class="fas fa-pencil-alt"></i>
            EDIT
          </a>
          <button class="btn btn-danger">
            <i class="fas fa-trash"></i>
            Hapus
          </button>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
